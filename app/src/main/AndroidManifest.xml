<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.langapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/toms___translation_svg_"
        android:label="@string/app_name"
        android:roundIcon="@drawable/toms___translation_svg_"
        android:supportsRtl="true"
        android:theme="@style/Theme.LangApp">
        <activity android:name=".PhraseActivity"
            android:label="Phrase"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".FamilyActivity"
            android:label="Family"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ColorActivity"
            android:label="Color"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".NumberActivity"
            android:label="Number"
            android:parentActivityName=".MainActivity"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>